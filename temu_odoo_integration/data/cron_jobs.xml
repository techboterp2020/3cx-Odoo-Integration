<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="ir_cron_temu_sync_orders" model="ir.cron">
            <field name="name">Temu: Sync New Orders</field>
            <field name="model_id" ref="model_temu_connector"/>
            <field name="state">code</field>
            <field name="code">model.sync_orders()</field>
            <field name="interval_number">15</field>
            <field name="interval_type">minutes</field>
        </record>

        <record id="ir_cron_temu_sync_inventory" model="ir.cron">
            <field name="name">Temu: Sync Inventory</field>
            <field name="model_id" ref="model_temu_connector"/>
            <field name="state">code</field>
            <field name="code">model.update_inventory()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">hours</field>
        </record>
    </data>
</odoo>